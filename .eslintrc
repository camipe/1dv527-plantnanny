{
  "extends": "airbnb-base",
  "rules": {
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
      "no-param-reassign": ["error", {
          "props": true,
          "ignorePropertyModificationsFor": ["err", "res", "req"]
      }],
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-console": "off"
  }
}